[alias]
    aa = add -A

    b = branch
    bd = branch -d
    bD = branch -D

    ci = commit
    cia = commit --amend
    ciaa = commit -a --amend
    cif = "!git ci --fixup HEAD"
    cifa = "!git ci -a --fixup HEAD"

    co = checkout
    com = checkout master

    cp = cherry-pick

    d = diff
    dc = diff --cached
    dh = diff HEAD~1
    dw = diff -w

    f = fetch -p --all --tags

    l1 = log --oneline                                                                                                                                                                                                                                   
    l1b = "!git log --oneline $(git merge-base HEAD @{-1}).."

    p = push
    pf = push -f

    r = "!git rebase -i $(git merge-base HEAD @{-1})"
    ras = "!git rebase -i $(git merge-base HEAD @{-1}) --autosquash"
    raac = "!git aa; git rc"
    ra = rebase --abort
    rc = rebase --continue

    st = status
                        
